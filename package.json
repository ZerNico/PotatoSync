{
  "name": "potatosync",
  "version": "2.0.0",
  "description": "PotatoSync REST API",
  "main": "dist/server.js",
  "scripts": {
    "watch-server": "nodemon --watch src -e ts,tsx --exec ts-node src/server.ts",
